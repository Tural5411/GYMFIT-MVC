<!DOCTYPE html>
<html lang="en">
<head>
    <title>ADMIN-PANEL</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/Areas/AdminPanel/AdminFront/LoginFront/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Areas/AdminPanel/AdminFront/LoginFront/css/main.css">
    <!--===============================================================================================-->
</head>
<body>
    <div class="limiter">
        <div class="container-login100" style="background-image: url(/Areas/AdminPanel/AdminFront/LoginFront/images/bg-01.jpg);">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                <form action="/AdminPanel/AdminHome/Login/" method="post" enctype="multipart/form-data" class="login100-form validate-form">
                    <span class="login100-form-title p-b-49">
                        ADMİN
                    </span>
                    @if (Session["adminId"]==null)
                    {
                        
                        <p>@ViewBag.alert</p>

                    }


                    <div class="wrap-input100 validate-input m-b-23" data-validate="Username is reauired">
                        <span class="label-input100">Email</span>
                        <input class="input100" type="text" name="email" placeholder="Emailini yaz">
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <span class="label-input100">Şifrə</span>
                        <input class="input100" type="password" name="password" placeholder="Şifrəni yaz">
                        <span class="focus-input100" data-symbol="&#xf190;"></span>
                    </div>

                    <div class="text-right p-t-8 p-b-31">
                        <a href="#">

                        </a>
                    </div>

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button class="login100-form-btn" type="submit">  DAXİL OL  </button>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>
    <div id="dropDownSelect1"></div>
</body>
</html>