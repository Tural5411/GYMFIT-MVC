@model IEnumerable<fitnessMVC.Model.tbl_mansoryPhoto>
@{
                Layout = null;
}
<div class="col-lg-12">
    <div class="row">
        @foreach (var x in Model)
        {
            <div class="grid-item col-lg-3 col-md-6 col-sm-12 p-0 m-0">
                <img class="img-fluid" src="@x.normalPhoto" alt="">
                <div class="text">
                    <h4>@x.Header</h4>
                </div>
            </div>
        }
    </div>
</div>